{
	"servers": {
		"mcpcli-wsl": {
			"type": "stdio",
			"command": "wsl",
			"args": [
				"npm",
				"run",
				"mcp",
				"--prefix=~/git/docker/project/lsshelper"
			],
			"env": {
				"NODE_TLS_REJECT_UNAUTHORIZED": "0"
			}
		},
		"mcpcli": {
			"type": "stdio",
			"command": "npm",
			"args": [
				"run",
				"mcp",
				"--prefix=${workspaceFolder}/unicornMCP"
			],
			"env": {
				"NODE_TLS_REJECT_UNAUTHORIZED": "0"
			}
		},
		"server": {
			"url": "http://localhost:3000/mcp",
			"type": "http"
		}
	},
	"inputs": []
}