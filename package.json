{
  "name": "lsshelper",
  "version": "1.0.0",
  "description": "1. Install [Tampermonkey](https://www.tampermonkey.net) 2. Enable developer mode in browser (if needed) 3. Click [here](https://github.com/vralfy/lsshelper/raw/master/lsshelper.user.js) to install",
  "main": "lsshelper.distribution.user.js",
  "scripts": {
    "mcp": "npm run cli --prefix ./unicornMCP -- -c ../mcp.config.json",
    "mcpserver": "npm run server --prefix ./unicornMCP -- -c ../mcpserver.config.json",
    "pull": "git submodule update --init --recursive && git pull --recurse-submodules",
    "push": "git submodule foreach git push && git push",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vralfy/lsshelper.git"
  },
